{
	"name": "hono-file-router",
	"version": "0.0.5",
	"description": "File-based router for easier route management for hono.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"private": false,
	"scripts": {
		"dev": "pkgroll --watch",
		"build": "pkgroll",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"hono",
		"file",
		"router"
	],
	"author": "thisjt",
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"hono": "^4.8.5"
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"@vitest/coverage-istanbul": "^3.2.4",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.3.0",
		"pkgroll": "^2.14.3",
		"prettier": "^3.6.2",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	}
}
